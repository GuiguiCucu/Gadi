{# src/Gadi/SiteBundle/Resources/views/Site/voir.html.twig #}
 
{% extends "GadiSiteBundle::layout.html.twig" %}
 
{% block title %}
  Lecture d'un enseignant - {{ parent() }}
{% endblock %}
 
{% block gadisite_body %}
 
  <h2>numero enseignant {{ enseignant.id }}</h2>
  <p>
 
  type enseignant : {{ enseignant.typeenseignant.intitule }}  <br/> 
  nom :{{ enseignant.prenomen}} {{ enseignant.nomen }}<br/> 
  <br/> 
  heure enseignant pour l'année {{ enseignant.heureannee}} <br/> 
 
  </p>
 
 
  <p>
    <a href="{{ path('gadisite_accueil') }}" class="btn">
      <i class="icon-chevron-left"></i>
      Retour à l'accueil
    </a>
  </p>
 
{% endblock %}